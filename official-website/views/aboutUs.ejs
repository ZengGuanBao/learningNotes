    <script>
        $(function(){
            $(".company-tabs li").click( function(){
                var i = $(this).index()
                $('#content .tab-content-item').hide().eq(i).show();
                $(".company-tabs li").eq(i).addClass("active").siblings().removeClass("active"); //当前列表高亮
