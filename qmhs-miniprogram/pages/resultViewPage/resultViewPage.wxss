/**resultViewPage.wxss**/
.redeemCode-result{
    padding:150rpx 50rpx 0;
}
.img-view{
    text-align: center;
}
.img{
    width: 136rpx;
    height: 136rpx;
}
.title{
    text-align:center;
    margin-top:30rpx;
    height:36rpx;
    font-size:36rpx;
    font-weight:600;
    color:rgba(51,51,51,1);
    line-height:36rpx;
}
.tip{
    margin-top:24rpx;
    text-align:center;
    font-size:26rpx;
    font-weight:400;
    color:rgba(153,153,153,1);
    line-height:26rpx;
}
.btn{
    width: 650rpx;
    height: 88rpx;
    background:linear-gradient(90deg,rgba(253,64,115,1) 0%,rgba(255,59,96,1) 100%);
    border-radius: 44rpx;
    text-align: center;
    font-size: 32rpx;
    font-weight: 600;
    color: rgba(255, 255, 255, 1);
    line-height: 88rpx;
    margin-top: 200rpx;
}
.btn-home{
    width:650rpx;
    height:88rpx;
    border-radius:44rpx;
    border:1rpx solid rgba(153,153,153,1);
    color:rgba(153,153,153,1);
    border-radius: 44rpx;
    text-align: center;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 88rpx;
    margin-top: 40rpx;
}