/**memberCenter.wxss**/
.member-center-container{
    position: relative;
    height: 100vh;
    background: #F7F8FC;
    box-sizing: border-box;
}
.member-center-container .wrapper{
    height: 100%;
}
.withdrawal-box{
    position: relative;
    height: 294rpx;
    padding: 10rpx 50rpx;
    color: #FFE2C1;
    font-size: 24rpx;
}
.withdrawal-box .img-member{
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    z-index: 0;
}
.withdrawal-money{
    font-size: 65rpx;
    margin-bottom: 5rpx;
}
.withdrawal-text, .withdrawal-money{
    position: relative;
    z-index: 1;
}
.btn-apply-withdrawal{
    position: absolute;
    width: 146rpx;
    height: 56rpx;
    top: 58rpx;
    right: 50rpx;
    line-height: 56rpx;
    text-align: center;
    border-radius: 28rpx;
    border: 1rpx solid #FFE2C1;
    z-index: 2;
}
.income-box, .fens-box, .activity-box{
    width: 710rpx;
    background: #fff;
    border-radius: 8rpx;
    margin: 0 auto 20rpx;
    padding: 0 30rpx 40rpx;
    box-sizing: border-box;
}
.income-box{
    position: relative;
    margin-top: -150rpx;
    z-index: 2;
}
.income-box>.title, .fens-box>.title, .activity-box>.title{
    display: flex;
    height: 112rpx;
    align-items: center;
    font-weight: 600;
}
.income-box>.title .title-left, .fens-box>.title .title-left, .activity-box>.title .title-left{
    flex: 1;
    position: relative;
}
.income-box>.title .title-left .yellow-block, .fens-box>.title .title-left .yellow-block, .activity-box>.title .title-left .yellow-block{
    position: absolute;
    display: inline-block;
    width: 8rpx;
    height: 36rpx;
    background: #FBBD77;
    border-radius: 0 3rpx 3rpx 0;
    top: 8rpx;
    left: -30rpx;
}
.income-box>.title .title-left .title-name, .fens-box>.title .title-left .title-name, .activity-box>.title .title-left .title-name{
    color: #333333;
    font-size: 32rpx;
}
.income-box>.title .title-left .img-show, .fens-box>.title .title-left .img-show, .activity-box>.title .title-left .img-show{
    width: 44rpx;
    height: 44rpx;
    margin-left: 4rpx;
}
.income-box>.title .title-right .more-text, .fens-box>.title .title-right .more-text, .activity-box>.title .title-right .more-text{
    font-size: 22rpx;
    color: #999;
}
.income-box>.title .title-right .img-arrow, .fens-box>.title .title-right .img-arrow, .activity-box>.title .title-right .img-arrow{
    width: 24rpx;
    height: 24rpx;
}
.income-list, .order-info .order-content{
    display: flex;
    margin-bottom: 62rpx;
    color: #999;
    font-size: 24rpx;
}
.income-list .income-list-item, .order-info .order-num-item{
    flex: 1;
}
.income-list .income-list-item .money, .order-info .num{
    color: #333;
    font-size: 32rpx;
    font-weight: 600;
    margin-bottom: 12rpx;
}
.order-info .order-title{
    color: #333;
    font-size: 32rpx;
    margin-bottom: 28rpx;
    font-weight: 600;
}
.order-info .order-content{
    margin-bottom: 0;
}
.fens-list{
    display: flex;
}
.fens-list-item{
    flex: 1;
}
.fens-list-item .fens-num{
    color: #333;
    font-size: 32rpx;
    font-weight: 600;
}
.fens-list-item .fens-text{
    color: #999;
    font-size: 24rpx;
    margin: 14rpx 0 10rpx;
}
.fens-list-item .fens-new{
    color: #E89B44;
    font-size: 20rpx;
}
.activity-box .task{
    padding-right: 10rpx;
    color: #999;
}
.activity-box .task .task-content{
    padding-left: 17rpx;
}
.target-month-title{
    color: #333;
    font-size: 26rpx;
    font-weight: 600;
    margin-bottom: 15rpx;
}
.target-month{
    color: #999;
}
.target-month, .target-month .yellow{
    font-size: 26rpx;
    line-height: 40rpx;
    vertical-align: top;
}
.target-month .yellow{
    color: #E89B44;
}
.activity-box .task{
    padding-top: 35rpx;
}
.task-title{
    color: #333;
    font-size: 26rpx;
}
.task-title .img-question{
    width: 24rpx;
    height: 24rpx;
    margin-left: 6rpx;
}
.task-progress-bar{
    position: relative;
    width: 100%;
    height: 12rpx;
    background: #F7F8FC;
    border-radius: 8rpx;
    margin: 60rpx 0 50rpx;
}
.task-progress-bar .progress-num{
    position: absolute;
    width: 0%;
    height: 100%;
    top: 0;
    left: 0;
    border-radius: 8rpx;
    background:linear-gradient(90deg,rgba(255,223,185,1) 0%,rgba(254,213,164,1) 100%);
}
.task-progress-bar .img-target{
    position: absolute;
    width: 44rpx;
    height: 44rpx;
    bottom: -16rpx;
    right: -22rpx;
}
.task-progress-bar .progress-text{
    position: absolute;
    min-width: 50rpx;
    right: 0;
    top: -40rpx;
    transform: translateX(50%);
    color: #E89B44;
    font-size: 22rpx;
    text-align: center;
}
.task-progress-bar .status{
    position: absolute;
    display: flex;
    width: 100%;
    top: 36rpx;
    left: 0;
    font-size: 22rpx;
    color: #E89B44
}
.task-progress-bar .status .status-text{
    flex: 1;
    color: #999;
}
.goods-box{
    width: 710rpx;
    background: #fff;
    border-radius: 8rpx;
    margin: 0 auto 20rpx;
    color: #999;
    font-size: 24rpx;
}
.goods-box>.goods-title{
    text-align: center;
    padding-top: 40rpx;
}
.goods-box>.goods-title .title-name{
    position: relative;
    color: #F5AB57;
    font-size: 36rpx;
}
.goods-box>.goods-title .sub-title-name{
    padding-top: 10rpx;
}
.goods-box>.goods-title .title-name::before, .goods-box>.goods-title .title-name::after{
    content: '';
    position: absolute;
    width: 32rpx;
    height: 4rpx;
    top: 50%;
    transform: translateY(-50%);
    background:linear-gradient(90deg,rgba(255,241,215,1) 0%,rgba(254,225,172,1) 100%);
    border-radius: 2rpx;
}
.goods-box>.goods-title .title-name::before{
    left: -47rpx;
}
.goods-box>.goods-title .title-name::after{
    background:linear-gradient(90deg,rgba(254,225,172,1) 0%,rgba(255,241,215,1) 100%);
    right: -47rpx;
}
.goods-box .goods-item{
    position: relative;
    display: flex;
    padding: 30rpx 20rpx;
}
.goods-box .goods-item:not(:last-child)::before{
    position: absolute;
    content: '';
    width: 394rpx;
    height: 1rpx;
    bottom: 1rpx;
    right: 20rpx;
    background: #eee;
}
.goods-box .goods-item .goods-left{
    width: 256rpx;
    height: 256rpx;
    border-radius:8px;
    overflow: hidden;
}
.goods-box .goods-item .goods-right{
    position: relative;
    flex: 1;
    height: 256rpx;
    min-width: 0;
    margin-left: 20rpx;
}
.goods-box .goods-item .goods-right .goods-detail{
    color: #333;
    font-size: 26rpx;
    height: 80rpx;
    line-height: 40rpx;
    margin-bottom: 20rpx;
}
.goods-box .goods-item .goods-right .cash-back{
    display: inline-block;
    height: 30rpx;
    padding: 0 10rpx;
    line-height: 30rpx;
    background: #FFD298;
    border-radius: 4rpx;
    color: #834B03;
    font-size: 20rpx;
}
.goods-box .goods-item .goods-right .goods-price{
    position: absolute;
    bottom: 30rpx;
    left: 0;
    padding-left: 20rpx;
    color: #FD4073;
    font-size: 34rpx;
}
.goods-box .goods-item .goods-right .goods-price::before{
    content: '￥';
    position: absolute;
    bottom: 5rpx;
    left: 3rpx;
    color: #FD4073;
    font-size: 20rpx;
}
.goods-origin-price{
    position: absolute;
    bottom: 0;
    left: 0;
    color: #999;
    font-size: 20rpx;
    text-decoration: line-through;
}
.img-share{
    position: absolute;
    width: 88rpx;
    height: 88rpx;
    bottom: -16rpx;
    right: 0;
}


/* 广告位 */
.ad-box{
    position: relative;
}
.ad-swiper{
    width: 710rpx;
    height: 164rpx;
    border-radius: 10rpx;
    margin: 0 20rpx 20rpx;
}
.ad-img{
    width:710rpx;
    height:164rpx;
    border-radius: 10rpx;
}
.banner-dots {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 10rpx;
    display: flex;
    justify-content: center;
}

.dot {
    width: 12rpx;
    height: 6rpx;
    background: rgba(255, 255, 255, 0.4);
    border-radius: 6rpx;
    margin: 0 5rpx;
    transition: all .6s;
}

.active {
    width: 18rpx;
    background: rgba(255, 255, 255, 1);
}
